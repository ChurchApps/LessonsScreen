{
  "expo": {
    "name": "Lessons.church",
    "slug": "LessonsScreen",
    "version": "3.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "church.lessons.screen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/images/tv_banner.png",
          "appleTVImages": {
            "icon": "./assets/images/icon-1280x768.png",
            "iconSmall": "./assets/images/icon-400x240.png",
            "iconSmall2x": "./assets/images/icon-800x480.png",
            "topShelf": "./assets/images/icon-1920x720.png",
            "topShelf2x": "./assets/images/icon-3840x1440.png",
            "topShelfWide": "./assets/images/icon-2320x720.png",
            "topShelfWide2x": "./assets/images/icon-4640x1440.png"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "lessons-screen",
          "organization": "churchapps"
        }
      ]
    ],
    "android": {
      "package": "church.lessons.screen",
      "versionCode": 4194327,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/images/splash.png"
      }
    },
    "ios": {
      "splash": {
        "image": "./assets/images/splash.png"
      }
    },
    "extra": {
      "eas": {
        "projectId": "3bba64bf-afb5-45af-8170-653bd011a340"
      }
    },
    "runtimeVersion": "3.0.0",
    "updates": {
      "url": "https://u.expo.dev/3bba64bf-afb5-45af-8170-653bd011a340",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 5000
    },
    "owner": "churchapps"
  }
}
